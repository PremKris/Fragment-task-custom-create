<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" >
<Dialog id="_IDDialog"
 title="Excel Upload">
        <VBox id="vbox1">
        <HBox> 
            <Label for="plant" showColon="true" text="PLANT" />
            <Input id="plant" text='plant' editable="false" />
            
        </HBox>
        <HBox>
            <Label for="sbu" showColon="true" text="SBU" />
            <Input id="sbu" text='SBU' />
        </HBox>
        <HBox>
            <Label for="sbg" showColon="true" text="SBG" />
            <Input id="sbg" text='SBG'/>
        </HBox>
        <HBox>
            <Button
            core:require="{ handler:'project1/ext/fragment/edit'}"
			text="Edit"
			press="handler.edit"
			class="sapUiSmallMarginBottom"
			ariaHasPopup="Dialog" />
            <Button
            core:require="{ handler:'project1/ext/fragment/edit'}"
			text="Cancel"
			press="handler.Cancelll"
			class="sapUiSmallMarginBottom"
			ariaHasPopup="Dialog" />
        </HBox>
        </VBox>
</Dialog>
</core:FragmentDefinition>